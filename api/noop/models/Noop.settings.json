{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "noop",
  "globalId": "Noops",
  "info": {
    "name": "noop-test",
    "description": "noop model"
  },
  "options": {
    "increments": true,
    "timestamps": ["createdAt", "updatedAt"],
    "comment": ""
  },
  "attributes": {
    "key": {
      "type": "string",
      "min": 5,
      "max": 20,
      "unique": true,
      "index": true,
      "required": true
    },
    "keyslug": {
      "type": "uid",
      "targetField": "key",
      "options": {
        "separator": "_",
        "decamelize": true
      }
    },
    "compName": {
      "type": "component",
      "component": "noop.strvalue",
      "required": true,
      "repeatable": false
    },
    "compValues": {
      "type": "dynamiczone",
      "components": ["noop.strvalue", "noop.numvalue"],
      "repeatable": true
    },
    "secure": {
      "type": "password",
      "private": true,
      "configurable": false
    },
    "file": {
      "plugin": "upload",
      "collection": "file",
      "via": "related"
    },
    "text": {
      "type": "text"
    },
    "richtext": {
      "type": "richtext"
    },
    "email": {
      "type": "email"
    },
    "integer": {
      "type": "integer",
      "max": 12,
      "min": 0
    },
    "biginteger": {
      "type": "biginteger"
    },
    "float": {
      "type": "float"
    },
    "decimal": {
      "type": "decimal"
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "datetime": {
      "type": "datetime"
    },
    "boolean": {
      "type": "boolean"
    },
    "enumeration": {
      "type": "enumeration",
      "enum": ["a", "b"]
    },
    "json": {
      "type": "json"
    },
    "uid": {
      "type": "uid"
    }
  }
}
